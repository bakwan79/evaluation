{
  "name": "evaluation",
  "version": "0.0.0",
  "private": true,
  "description": "Evaluation",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.18.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "build": "vue-tsc -p tsconfig.build.json --noEmit && vite build --emptyOutDir",
    "dev": "vite",
    "lint": "eslint --ext .js,.ts,.vue src",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest run --coverage",
    "test:watch": "vitest --",
    "eslint:ci": "eslint './**/*.{ts,js}'",
    "eslint": "eslint './**/*.{ts,js}' --fix",
    "prepare": "husky install",
    "prettier:check": "prettier --check '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}'",
    "prettier:format": "prettier --write '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}'"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.5.1",
    "@typescript-eslint/parser": "6.7.4",
    "@vitejs/plugin-vue": "4.4.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/test-utils": "2.4.1",
    "@vitest/coverage-istanbul": "0.34.6",
    "eslint": "8.50.0",
    "eslint-plugin-vue": "9.17.0",
    "jsdom": "22.1.0",
    "typescript": "5.2.2",
    "vite": "4.4.10",
    "vitest": "0.34.6",
    "vitest-sonar-reporter": "0.4.1",
    "vue-tsc": "1.8.15",
    "@types/sinon": "10.0.18",
    "sinon": "16.0.0",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "@prettier/plugin-xml": "3.2.1",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-java": "2.3.1",
    "prettier-plugin-packagejson": "2.4.6"
  },
  "dependencies": {
    "vue": "3.3.4",
    "axios": "1.5.1",
    "vue-router": "4.2.5"
  }
}
